{
    "name": "checkthisfile",
    "displayName": "checkThisFile",
    "description": "Checks for the harmful code snippets",
    "version": "0.0.1",
    "publisher": "Manish",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.checkMe",
            "title": "checkMe",
            "category": "Markdown",
            "icon": "./media/inf.svg"
        },
        {
            "command": "extension.again",
            "title": "again",
            "icon": "index.png"
        }],

        "menus": {
            "editor/title": [{
                "command": "extension.checkMe",
                "title":"checkMe",
                "group":"navigation"
            },{
                "command": "extension.again",
                "title": "again",
                "group": "navigation"
            }]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
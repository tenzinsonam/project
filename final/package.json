{
    "name": "final",
    "displayName": "",
    "description": "",
    "version": "0.0.1",
    "publisher": "qwer",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.check",
            "title": "Check",
            "icon": "media/index.jpeg"
            
        },{
            "command": "extension.replace",
            "title": "Replace",
            "icon": "media/index.png"
        }],
        "menus": {
            "editor/title": [{
                "command": "extension.replace",
                "group": "navigation"
            },{
                "command": "extension.check",
                "group": "navigation"
            }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
